{
    "name": "marble",
    "displayName": "MARBLE",
    "description": "Marble programming language syntax highlighting",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.64.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "marble",
            "aliases": ["Marble", "marble"],
            "extensions": [".marble"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "marble",
            "scopeName": "source.marble",
            "path": "./syntaxes/marble.tmLanguage.json"
        }]
    }
}